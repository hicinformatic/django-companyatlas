# Environment variables for django-companyatlas
# Copy this file to .env and fill in your values

# Django settings
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database (if not using SQLite)
# DATABASE_URL=postgresql://user:password@localhost/dbname

# CompanyAtlas Backends
# Format: COMPANYATLAS_{BACKEND_NAME}_{KEY}
# Example for INSEE:
COMPANYATLAS_INSEE_API_KEY=your-insee-api-key-here
# COMPANYATLAS_INSEE_BASE_URL=

# Other backends (see python-companyatlas/env.example for full list)
# COMPANYATLAS_ENTDATAGOUV_BASE_URL=
# COMPANYATLAS_PAPPERS_API_KEY=

